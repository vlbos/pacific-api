{
  "repository": "https://github.com/polkadot-js/api",
  "author": "",
  "license": "",
  "type": "commonjs",
  "resolutions": {
    "typescript": "^4.2.3"
  },
  "scripts": {
    "typegen": "yarn generate:defs && yarn generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package pacific-api/interfaces --input ./packages/interfaces",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package pacific-api/interfaces --endpoint ./pacific.json --output ./packages/interfaces --strict",
    "test": "polkadot-dev-run-test --coverage ",
    "test:api": "polkadot-dev-run-test packages/test/api/api.test.ts",
    "test:order": "polkadot-dev-run-test packages/test/seaport/orders.test.ts  ",
    "test:transfer": "polkadot-dev-run-test packages/test/seaport/ownersAndTransfers.test.ts  ",
    "t:contract": "polkadot-dev-run-test packages/test/seaport/contracts-rust.spec.ts  ",
    "t:erc20": "polkadot-dev-run-test packages/test/seaport/contracts-erc20.spec.ts ",
    "t:msig": "polkadot-dev-run-test packages/test/seaport/contracts-multisig.spec.ts ",
    "t:erc721": "polkadot-dev-run-test packages/test/seaport/contracts-erc721.spec.ts  ",
    "t:onex": "polkadot-dev-run-test packages/test/seaport/orders.test.ts --runInBand --detectOpenHandles --forceExit",
    "ta:o": "polkadot-dev-run-test packages/api/test/orderbook.rpc.spec.ts",
    "ta:t": "polkadot-dev-run-test packages/api/test/orderbook.tx.spec.ts",
    "tw:t": "polkadot-dev-run-test packages/api/test/wyvernExchange.tx.spec.ts",
    "test:watch": "polkadot-dev-run-test --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/register": "^7.13.8",
    "@babel/runtime": "^7.13.10",
    "@polkadot/dev": "^0.62.68",
    "@types/jest": "^26.0.21",
    "copyfiles": "^2.4.1"
  },
  "version": "0.0.1",
  "dependencies": {
    "@polkadot/api": "^7.0.3-2",
    "@polkadot/api-contract": "^7.0.3-2",
    "@polkadot/types": "^7.0.3-2",
    "@polkadot/types-augment": "^7.1.1",
    "bignumber.js": "^9.0.1",
    "blakejs": "^1.1.1",
    "bn.js": "^5.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "query-string": "^7.0.1",
    "uuid": "^8.3.2"
  }
}
